{
  "name": "gulp-flowtype",
  "version": "1.1.0",
  "description": "Run Facebook's Flow in your gulp pipeline",
  "keywords": [
    "gulp",
    "flowtype"
  ],
  "main": "lib/index.js",
  "author": {
    "name": "Charlie Dowler",
    "url": "https://github.com/charliedowler"
  },
  "repository": "charliedowler/gulp-flowtype",
  "scripts": {
    "lint": "npm run lint:js && npm run lint:flow",
    "lint:js": "jshint **.js",
    "lint:flow": "flow check --lib ./declarations/",
    "test": "babel -d lib index.js; istanbul test _mocha --report html -- test/*.js --reporter spec",
    "coveralls": "istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- -R spec && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "prepublish": "babel -d lib index.js"
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "event-stream": "^4.0.1",
    "flow-reporter": "~0.1.0",
    "gulp-util": "~3.0.1",
    "log-symbols": "^2.2.0",
    "q": "^1.5.1",
    "through2": "~3.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "coveralls": "~2.11.5",
    "flow-bin": "^0.51.0",
    "istanbul": "^0.4.5",
    "jshint": "^2.9.7",
    "mocha": "~5.2.0",
    "should": "~10.0.0",
    "@babel/preset-flow": "^7.0.0"
  },
  "peerDependencies": {
    "flow-bin": ">=0.39.0 <1.0"
  },
  "engines": {
    "node": ">=4.0",
    "npm": ">=3.0.10"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-flow"
    ],
    "plugins": [
      "@babel/plugin-syntax-import-meta",
      "@babel/plugin-proposal-json-strings"
    ]
  },
  "license": "MIT"
}
